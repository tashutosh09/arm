{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "bin": "dist/index.js",
  "scripts": {
    "build": "gulp scripts",
    "clean-up": "rm -rf ../server-logs && rm -rf ../server-temp && rm -rf dist",
    "ready-for-build": "npm run build",
    "start": "npm run ready-for-build && node dist/index.js ../config_server/server.config.json",
    "start-low-mem": "npm run ready-for-build && node --max-old-space-size=30 dist/index.js '../config_server/server.config.json'",
    "clean-start": "npm run clean-up && npm run start",
    "inspect": "npm run build && node inspect dist/index.js '../config_server/server.config.json'",
    "start-live": "npm run build && nodemon dist/index.js '../config_server/server.config.json'",
    "test": "mocha --reporter spec --compilers ts:ts-node/register 'test/**/*.test.ts'"
  },
  "pkg": {
    "assets": "dist/public/**/*",
    "targets": [
      "node8-linux"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/chai": "^3.4.34",
    "@types/chai-http": "0.0.29",
    "@types/debug": "0.0.29",
    "@types/express": "^4.0.33",
    "@types/mocha": "^2.2.32",
    "@types/morgan": "^1.7.32",
    "@types/mysql": "^2.15.2",
    "@types/node": "^6.0.46",
    "@types/socket.io": "^1.4.31",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-typescript": "^3.1.1",
    "mocha": "^3.1.2",
    "ts-node": "^3.3.0",
    "typescript": "^2.0.6"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.7.1",
    "cors": "^2.8.5",
    "cryptr": "^2.0.0",
    "debug": "^2.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^2.3.1",
    "express-session": "^1.17.1",
    "find-remove": "^1.2.0",
    "fs-extra": "^5.0.0",
